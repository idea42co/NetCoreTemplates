{
  "name": "angularapp",
  "version": "0.0.0",
  "scripts": {
    "docker-build": "docker build -t angularwebapp .",
    "docker-run": "docker run -d -p 4200:4200 --name angularapp angularwebapp",
    "docker-stop": "docker container stop angularapp || true",
    "docker-remove": "docker container rm angularapp || true",
    "docker": "npm-run-all docker-build docker-stop docker-remove docker-run",
    "pre-build": "rimraf -r wwwroot || true",
    "post-build": "node-mv dist wwwroot",
    "ng": "ng",
    "start": "ng serve",
    "build-js": "ng build",
    "build": "npm-run-all pre-build build-js post-build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "node-mv": "^0.1.3"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-angular": "~0.6.8",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
